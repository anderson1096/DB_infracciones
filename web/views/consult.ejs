<% include header %>
<body background-color = 'teal'>
  <% include nav-bar %>
  <div class="container">
    <table class="bordered centered highlight responsive-table">
        <thead class="center s6">
          <tr>
              <th data-field="id">Numero</th>
              <th data-field="name">Fecha</th>
              <th data-field="price">Valor</th>
              <th data-field="price">Art Sancionado</th>
              <th data-field="price">Lugar Multa</th>
              <th data-field="price">Cedula</th>
              <th data-field="price">Nombre</th>
              <th data-field="price">Apellido</th>
              <th data-field="price">Vehiculo</th>

          </tr>
        </thead>

        <tbody class="center">

          <% multa.forEach(function(mult){ %>
            <tr>
              <td><%= mult.id_multa %></td>
              <td><%= mult.fecha_multa %></td>
              <td>$<%= mult.importe_multa %></td>
              <td><%= mult.articulo_multa %></td>
              <td>Pereira</td>
              <td><%= mult.id_persona %></td>
              <td><%= persona[0].nombre_persona %></td>
              <td><%= persona[0].apellido_persona %></td>
              <td><%= vehiculos[0] %></td>
            </tr>
          <tr>
          <% }) %>

        </tbody>
      </table>
    </div>
<% include footer %>
